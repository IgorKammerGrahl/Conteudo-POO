package Eletronico;

public class Main {
	public static void main(String[] args) {
		EquipamentoEletronico ee = new EquipamentoEletronico();
		ee.setConsumo(100);
		ee.setTensao(220);
		System.out.println(ee);
		
		SistemaEletronico ss = new SistemaEletronico();
		ss.setConsumo(100);
		ss.setTensao(220);
		ss.setMarca("Sony");
		ss.setModelo("S3");
		
		SistemaDeCideo sv = new SistemaDeVideo();
		ss.setConsumo(100);
		ss.setTensao(220);
		ss.setMarca("Sony");
		ss.setModelo("S3");
	}
	

}
